{
  "name": "cypress-qa-makeyourjodi-live",
  "version": "1.0.0",
  "main": "cypress.config.js",
  "scripts": {
    "clean:reports": "rimraf cypress/reports/* cypress/screenshots/* cypress/videos/*",
    "merge:reports": "mochawesome-merge cypress/reports/*.json > cypress/reports/merged-report.json",
    "generate:report": "marge cypress/reports/merged-report.json --reportDir cypress/reports/html --inline",
    "cy:run": "npm run clean:reports && cypress run",
    "cy:open": "npm run clean:reports && cypress open",
    "test": "npm run clean:reports && cypress run",
    "test:chrome": "npm run clean:reports && cypress run --browser chrome",
    "test:spec": "npm run clean:reports && cypress run --spec"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "cypress": "^14.5.3",
    "cypress-mochawesome-reporter": "^4.0.0",
    "cypress-xpath": "^2.0.1",
    "dotenv": "^17.2.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0",
    "rimraf": "^6.0.1"
  }
}
